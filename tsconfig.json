{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "none",
        "outFile": "./imis-plus.js",
        "strict": true,
        "target": "es5",
        "noImplicitAny": true,
        "allowJs": false,
        "lib": [
            "es2015",
            "DOM"
        ],
        "skipLibCheck": true /* Skip type checking all .d.ts files. */
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}